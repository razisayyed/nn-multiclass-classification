{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "NN Multiclass Classification",
  "version": "0.1.2",
  "identifier": "edu.najah.nn-multiclass-classification",
  "build": {
    "beforeDevCommand": "pnpm dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "NN Multiclass Classification",
        "width": 1024,
        "height": 900,
        "resizable": true,
        "acceptFirstMouse": true,
        "center": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}